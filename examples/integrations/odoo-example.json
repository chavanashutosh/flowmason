{
  "name": "Odoo Integration Examples",
  "description": "Examples of Odoo ERP integration configurations",
  "examples": [
    {
      "name": "Get Invoices",
      "description": "Retrieve invoices from Odoo",
      "brick_type": "odoo",
      "config": {
        "url": "https://your-odoo-instance.com",
        "database": "your-database-name",
        "username": "your-username",
        "password": "your-password",
        "operation": "get_invoices"
      },
      "input_example": {}
    },
    {
      "name": "Create Invoice",
      "description": "Create a new invoice in Odoo",
      "brick_type": "odoo",
      "config": {
        "url": "https://your-odoo-instance.com",
        "database": "your-database-name",
        "username": "your-username",
        "password": "your-password",
        "operation": "create_invoice"
      },
      "input_example": {
        "invoice_data": {
          "partner_id": 1,
          "invoice_date": "2024-01-15",
          "invoice_line_ids": [
            {
              "product_id": 1,
              "quantity": 10,
              "price_unit": 100.0
            }
          ]
        }
      }
    },
    {
      "name": "Get Products",
      "description": "Retrieve products from Odoo",
      "brick_type": "odoo",
      "config": {
        "url": "https://your-odoo-instance.com",
        "database": "your-database-name",
        "username": "your-username",
        "password": "your-password",
        "operation": "get_products"
      },
      "input_example": {}
    }
  ]
}

